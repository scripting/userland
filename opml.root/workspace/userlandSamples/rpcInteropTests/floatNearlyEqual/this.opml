	<body>
		<outline text="on floatNearlyEqual (a, b)">
			<outline text="4/16/01; 2:18:29 PM by JES" isComment="true">
				<outline text="Tests whether two floating point numbers are close enough to be considered equal."/>
				<outline text="This is necessary because binary representations of floating point numbers do not map exactly to decimal representations, so converstions may introduce small, but acceptible errors."/>
				</outline>
			<outline text="if a == b">
				<outline text="return (true)"/>
				</outline>
			<outline text="local (epsilon = 0.0000001)"/>
			<outline text="local (nearlyEqual = abs (a - b) &lt;= (abs (a) * epsilon))"/>
			<outline text="return (nearlyEqual)"/>
			</outline>
		</body>
