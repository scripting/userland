	<body>
		<outline text="on echoIntegerArray (server, port, protocol)">
			<outline text="local (anIntegerArray = {})"/>
			<outline text="bundle //generate the array">
				<outline text="local (i)"/>
				<outline text="for i = 1 to 3">
					<outline text="local (anInteger = random (-5000, 5000))"/>
					<outline text="anIntegerArray = anIntegerArray + anInteger"/>
					</outline>
				</outline>
			<outline text=""/>
			<outline text="local (result, params = {&quot;inputIntegerArray&quot;:anIntegerArray})"/>
			<outline text="result = xml.rpc (server, port, &quot;interopEchoTests.echoIntegerArray&quot;, @params, protocol:protocol, soapAction:&quot;/interopEchoTests&quot;)"/>
			<outline text=""/>
			<outline text="if typeOf (result) != listType">
				<outline text="scriptError (&quot;The call failed because the response was not an array.&quot;)"/>
				</outline>
			<outline text="local (i, ct = sizeOf (result))"/>
			<outline text="if ct != sizeOf (anIntegerArray)">
				<outline text="scriptError (&quot;The call failed because the size of the request array is different from the size of the response array.&quot;)"/>
				</outline>
			<outline text="for i = 1 to ct">
				<outline text="if result[i] != anIntegerArray[i]">
					<outline text="scriptError (&quot;The call failed because the response was different from the request. We sent &quot; + anIntegerArray[i] + &quot; for element &quot; + i + &quot;, and the server returned &quot; + result[i] + &quot;.&quot;)"/>
					</outline>
				</outline>
