	<body>
		<outline text="on echoStructArray (inputStructArray)">
			<outline text="if defined (adrRequest) //set some directives if we're processing a SOAP request">
				<outline text="if defined (adrRequest^)">
					<outline text="adrRequest^.customStructType = &quot;SOAPStruct&quot;"/>
					<outline text="adrRequest^.customNamespace = &quot;urn&quot;"/>
					<outline text="adrRequest^.customNamespaceURI = &quot;http://www.xmethods.com/service&quot;"/>
					</outline>
				</outline>
			<outline text="if typeOf (inputStructArray) != listType and typeOf (inputStructArray) != tableType">
				<outline text="scriptError (&quot;Can't process the request because the input parameter was not an array of structs.&quot;)"/>
				</outline>
			<outline text="local (returnArray = {})"/>
			<outline text="for i = 1 to sizeOf (inputStructArray)">
				<outline text="if typeOf (inputStructArray[i]) != tableType">
					<outline text="scriptError (&quot;Can't process the request because element &quot; + i + &quot; (1-based) is not a struct.&quot;)"/>
					</outline>
				<outline text="returnArray[i] = inputStructArray[i]"/>
				</outline>
			<outline text="return (returnArray)"/>
			</outline>
		</body>
