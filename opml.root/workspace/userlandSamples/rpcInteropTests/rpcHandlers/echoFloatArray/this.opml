	<body>
		<outline text="on echoFloatArray (inputFloatArray)">
			<outline text="if typeOf (inputFloatArray) != listType">
				<outline text="scriptError (&quot;Can't process the request because the input parameter is not an array of floats.&quot;)"/>
				</outline>
			<outline text="for i = 1 to sizeOf (inputFloatArray)">
				<outline text="if typeOf (inputFloatArray[i]) != doubleType">
					<outline text="scriptError (&quot;Can't process the request because element &quot; + i + &quot; (1-based) is not a float.&quot;)"/>
					</outline>
				</outline>
			<outline text="return (inputFloatArray)"/>
			</outline>
		</body>
