	<body>
		<outline text="on echoIntegerArray (inputIntegerArray)">
			<outline text="if typeOf (inputIntegerArray) != listType">
				<outline text="scriptError (&quot;Can't process the request because the input parameter is not an array of integers.&quot;)"/>
				</outline>
			<outline text="for i = 1 to sizeOf (inputIntegerArray)">
				<outline text="if typeOf (inputIntegerArray[i]) != longType">
					<outline text="scriptError (&quot;Can't process the request because element &quot; + i + &quot; (1-based) is not an integer.&quot;)"/>
					</outline>
				</outline>
			<outline text="return (inputIntegerArray)"/>
			</outline>
		</body>
