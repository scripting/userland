	<body>
		<outline text="on echoStringArray (inputStringArray)">
			<outline text="if typeOf (inputStringArray) != listType">
				<outline text="scriptError (&quot;Can't process the request because the input parameter was not an array of strings.&quot;)"/>
				</outline>
			<outline text="for i = 1 to sizeOf (inputStringArray)">
				<outline text="if typeOf (inputStringArray[i]) != stringType">
					<outline text="scriptError (&quot;Can't process the request because element &quot; + i + &quot; (1-based) is not a string.&quot;)"/>
					</outline>
				</outline>
			<outline text="return (inputStringArray)"/>
			</outline>
