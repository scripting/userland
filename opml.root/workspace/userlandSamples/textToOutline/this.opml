	<body>
		<outline text="on textToOutline ()">
			<outline text="Changes" isComment="true">
				<outline text="2/25/15; 12:05:19 PM by DW">
					<outline text="Take a string of text separated by \n chars and break it up into an outline."/>
					</outline>
				</outline>
			<outline text="local (s = string (scratchpad.exampletext), adroutline = @scratchpad.exampleoutline)"/>
			<outline text="new (outlinetype, adroutline)"/>
			<outline text="target.set (adroutline)"/>
			<outline text="loop">
				<outline text="line = string.nthfield (s, &quot;\n&quot;, 1)"/>
				<outline text="op.insert (line, down)"/>
				<outline text="s = string.delete (s, 1, sizeof (line) + 1)"/>
				<outline text="if sizeof (s) == 0">
					<outline text="break"/>
					</outline>
				</outline>
			</outline>
		<outline text="bundle //test code">
			<outline text="textToOutline ()"/>
			</outline>
