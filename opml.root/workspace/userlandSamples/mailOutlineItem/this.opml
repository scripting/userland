					</outline>
				</outline>
			<outline text=""/>
			<outline text="local (adroutline = address (window.frontmost ()))"/>
			<outline text="if typeof (adroutline^) != outlinetype">
				<outline text="scriptError (&quot;This command only works with outline windows.&quot;)"/>
				</outline>
			<outline text="if dialog.ask (&quot;Enter subject:&quot;, @scratchpad.lastBulletinSubject)">
				<outline text="local (s = string.htmlToEmail (op.getlinetext ()) + string.filledstring (&quot;\r\n &quot;, 3))"/>
				<outline text="tcp.sendmail (recipient, scratchpad.lastBulletinSubject, s)"/>
				</outline>
