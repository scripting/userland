	<body>
		<outline text="on fasterTextBuilding ()">
			<outline text="Changes" isComment="true">
				<outline text="8/27/11; 9:55:36 AM by DW">
					<outline text="We discovered a faster way to build blocks of text, something web applications do a lot. Two methods below, one is faster, as you'll see by running this script."/>
					</outline>
				</outline>
			<outline text="local (ctloops = 25000)"/>
			<outline text="on fastmethod ()">
				<outline text="local (htmltext = &quot;&quot;, indentlevel = 0, i)"/>
				<outline text="on add (s)">
					<outline text="htmltext = htmltext + (string.filledstring (&quot;\t&quot;, indentlevel) + s + &quot;\r&quot;);"/>
					</outline>
				<outline text="indentlevel = 1"/>
				<outline text="for i = 1 to ctloops">
					<outline text="add (string.filledstring (&quot;123456789 &quot;, random (1, 10)))"/>
					</outline>
				<outline text="return (htmltext)" created="Thu, 12 Jun 2008 19:01:51 GMT"/>
				</outline>
			<outline text="on slowmethod ()">
				<outline text="local (htmltext = &quot;&quot;, indentlevel = 0, i, tc = clock.ticks ())"/>
				<outline text="on add (s)">
					<outline text="htmltext = htmltext + string.filledstring (&quot;\t&quot;, indentlevel) + s + &quot;\r&quot;;"/>
					</outline>
				<outline text="indentlevel = 1"/>
				<outline text="for i = 1 to ctloops">
					<outline text="add (string.filledstring (&quot;123456789 &quot;, random (1, 10)))"/>
					</outline>
				<outline text="return (htmltext)" created="Thu, 12 Jun 2008 19:01:51 GMT"/>
				</outline>
			<outline text="local (tc = clock.ticks ())"/>
			<outline text="fastmethod ()"/>
			<outline text="fastticks = clock.ticks () - tc; tc = clock.ticks ()"/>
			<outline text="slowmethod ()"/>
			<outline text="slowticks = clock.ticks () - tc"/>
			<outline text="dialog.alert (&quot;fast = &quot; + fastticks + &quot; ticks. slow = &quot; + slowticks + &quot;.&quot;)"/>
			</outline>
		<outline text="bundle //test code">
			<outline text="fasterTextBuilding ()"/>
			</outline>
		</body>
